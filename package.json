{
  "name": "ariamusic",
  "version": "1.00",
  "description": "Aria Music is a music bot for Discord, written in JavaScript using the Discord.js, lavalink-client (Lavalink) library.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "dotenvx run -- node launcher.js",
    "start:dev": "dotenvx run -- node src/index.js",
    "start:prod": "dotenvx run -- node dist/index.js",
    "build": "node build.js",
    "build:prod": "npm run build && echo 'Production build ready!'",
    "db:push": "npx prisma db push --schema=src/configure/schema.prisma",
    "db:generate": "npx prisma generate --schema=src/configure/schema.prisma",
    "db:studio": "npx prisma studio --schema=src/configure/schema.prisma",
    "db:migrate": "npx prisma migrate dev --name init --schema=src/configure/schema.prisma",
    "start:panel": "npm run start:prod",
    "setup:prod": "npm i && npm run db:generate && npm run db:push && npm run build:prod",
    "launch": "dotenvx run -- node launcher.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moulendu-Chowley/AriaMusic"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "lavalink-client",
    "ariamusic",
    "javascript",
    "prisma"
  ],
  "author": "Moulendu Chowley",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Moulendu-Chowley/AriaMusic/issues"
  },
  "homepage": "https://github.com/Moulendu-Chowley/AriaMusic#readme",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@prisma/client": "^6.16.3",
    "@top-gg/sdk": "^3.1.6",
    "discord.js": "^14.22.1",
    "genius-lyrics": "^4.4.7",
    "googleapis": "^160.0.0",
    "lavalink-client": "^2.6.1",
    "prisma": "^6.16.3",
    "node-system-stats": "^2.0.5",
    "signale": "^1.4.0",
    "topgg-autoposter": "^2.0.2",
    "undici": "^7.16.0",
    "zod": "^4.1.11"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  },
  "devDependencies": {
    "esbuild": "^0.25.10"
  }
}
